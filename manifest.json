{
    "manifest_version" : 2,
    "name"           : "mdash",
    "version"        : "0.9.2",
    "description"    : "A minimal dashboard replacing the \"New Tab\" page which syncs with your bookmarks.",
    "icons": {
        "16":  "img/icon-16.png",
        "48":  "img/icon-48.png",
        "128": "img/icon-128.png"
    },
    "permissions": [
        "bookmarks",
        "contextMenus"
    ],
    "chrome_url_overrides" : {
        "newtab" : "html/dashboard.html"
    }
}
